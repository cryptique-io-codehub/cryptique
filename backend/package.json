{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "mocha test/**/*.test.js --timeout 30000",
    "test:vector": "mocha test/vectorDatabase.test.js --timeout 30000",
    "test:vector:verbose": "VERBOSE=true mocha test/vectorDatabase.test.js --timeout 30000",
    "test:vector:no-cleanup": "CLEANUP=false mocha test/vectorDatabase.test.js --timeout 30000",
    "vector:setup": "node scripts/setupVectorDatabase.js",
    "vector:test": "node scripts/testVectorInfrastructure.js",
    "vector:backup:full": "node scripts/vectorBackupRestore.js full",
    "vector:backup:incremental": "node scripts/vectorBackupRestore.js incremental",
    "vector:backup:list": "node scripts/vectorBackupRestore.js list",
    "vector:backup:status": "node scripts/vectorBackupRestore.js status",
    "vector:restore": "node scripts/vectorBackupRestore.js restore",
    "vector:monitoring:start": "node -e \"const {vectorMonitoringService} = require('./services/vectorMonitoringService'); vectorMonitoringService.start().then(() => console.log('Monitoring started')).catch(console.error);\"",
    "vector:health": "node -e \"const {vectorDatabase} = require('./config/vectorDatabase'); vectorDatabase.initialize().then(() => vectorDatabase.healthCheck()).then(console.log).catch(console.error).finally(() => vectorDatabase.shutdown());\"",
    "vector:stats": "node -e \"const {vectorDatabase} = require('./config/vectorDatabase'); vectorDatabase.initialize().then(() => vectorDatabase.getStats()).then(console.log).catch(console.error).finally(() => vectorDatabase.shutdown());\"",
    "vector:init": "npm run vector:setup && npm run vector:test",
    "vector:full-setup": "npm run vector:setup && npm run vector:test && npm run vector:backup:full"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@duneanalytics/client-sdk": "^0.2.5",
    "@google/generative-ai": "^0.20.0",
    "axios": "^1.8.4",
    "backend": "file:",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.1.9",
    "puppeteer": "^24.5.0",
    "stripe": "^18.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "chalk": "^4.1.2",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1"
  }
}
