{
  "version": 2,
  "name": "cryptique-backend-optimized",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/index.js"
    },
    {
      "src": "/api/analytics/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/teams/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/websites/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/smartcontracts/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/sdk/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/onchain/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/campaigns/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/transactions/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_URI": "@mongodb_uri",
    "GEMINI_API_KEY": "@gemini_api_key"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}
