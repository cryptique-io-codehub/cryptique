{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sdk/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, x-cryptique-site-id",
        "Access-Control-Max-Age": "86400"
      },
      "continue": true
    },
    {
      "src": "/api/sdk/(.*)",
      "methods": ["OPTIONS"],
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/sdk/update-all-analytics-stats-hourly",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/sdk/update-all-analytics-stats-daily",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/sdk/update-all-analytics-stats-weekly",
        "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/sdk/update-all-analytics-stats-monthly",
      "schedule": "*/30 * * * *"
    }
  ]
}
